{
    "name": "aanbieders/api",
    "description": "Package to recover information of the Aanbieders.be backend using the Aanbieders API",
    "version": "0.0.1",
    "keywords": ["Ixudra", "Laravel", "Aanbieders", "API"],
    "homepage": "http://aanbieders.be",
    "license": "MIT",

    "authors": [
        {
            "name": "Jan Oris",
            "email": "jan.oris@ixudra.be"
        }
    ],

    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*"
    },

    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        
        "psr-0": {
            "Aanbieders\\Api\\": "src/"
        }
    },

    "minimum-stability": "stable"
}
