{
    "name": "aanbieders/laravel-api",
    "description": "Laravel package to recover information of the Aanbieders.be Comparison service provider, using the Aanbieders API",
    "version": "0.1.8",
    "keywords": ["Laravel", "Aanbieders", "API"],
    "homepage": "http://www.aanbieders.be",
    "license": "MIT",

    "authors": [
        {
            "name": "Jan Oris",
            "email": "jan.oris@ixudra.be"
        },
        {
            "name": "Evert Engelen",
            "email": "evert@aanbieders.be"
        }
    ],

    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "illuminate/config": "4.1.*",
        "everto/aanbieders-api-client": "0.1.*",
        "ixudra/curl": "0.1.*"
    },

    "require-dev": {
        "phpunit/phpunit" : "4.0.*",
        "mockery/mockery": "0.9.*"
    },

    "autoload": {
        "classmap": [
            "src/migrations"
        ],

        "psr-0": {
            "Aanbieders\\Api\\": "src/"
        }
    },

    "minimum-stability": "stable"
}
